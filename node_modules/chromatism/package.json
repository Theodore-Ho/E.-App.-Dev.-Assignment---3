{
  "name": "chromatism",
  "version": "3.0.0",
  "description": "A simple set of utility functions for colours.",
  "main": "dist/chromatism.umd.js",
  "browser": "dist/chromatism.cjs.js",
  "module": "src/operations/index.js",
  "typings": "./index.d.ts",
  "repository": "toish/chromatism",
  "scripts": {
    "test": "BABEL_ENV=test mocha --compilers js:babel-core/register",
    "lint": "standard \"src/**/*.js\" \"test/**/*.js\"",
    "build": "rollup -c",
    "format": "standard --fix \"src/**/*.js\" \"test/**/*.js\""
  },
  "keywords": [
    "color",
    "colour",
    "utility",
    "hue",
    "chroma",
    "CMYK",
    "RGB",
    "HSL"
  ],
  "author": "Toish <toish@toi.sh>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "mocha": "^3.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "standard": "10.0.2",
    "uglify-es": "3.0.19"
  },
  "false": {},
  "browserslist": [
    "ie >= 9",
    "last 2 versions"
  ]
}
